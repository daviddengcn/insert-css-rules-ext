{
    "name": "Insert CSS Rules",
    "short_name": "ICR",
    "description": "Automatically insert CSS rules into a web page.",
    "permissions": [
        "scripting",
        "storage",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "128":"icon-128.png"
    },
    "content_scripts": [],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
        "default_icon": "icon-128.png"
    },
    "version": "1.2",
    "manifest_version": 3
}
